{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{.Folder}}{{if .Root}} | root{{end}} - {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ECEFF4",
          "background": "#4C566A",
          "template": " <#D08770, transparent>\udb86\udc53</>"
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ECEFF4",
          "background": "#ff8080",
          "template": " Ôåà {{ .Context }} "
        },
        {
          "type": "session",
          "style": "diamond",
          "background": "#4C566A",
          "foreground": "#D8DEE9",
          "leading_diamond": "",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "<#58647A, transparent>ÓÇ∞</>"
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<#4C566A, transparent>ÓÇ∞</>",
          "foreground": "#ECEFF4",
          "background": "#3B4252",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " <#4C566A, transparent>\ue0b1</> ",
            "home_icon": "üè†",
            "style": "agnoster"
          }
        },
        {
          "type": "angular",
          "style": "diamond",
          "trailing_diamond": "",
          "foreground": "#5E81AC",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#6CA35E",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#BF616A",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#EBCB8B",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} ÓÇ± {{ end }}{{ .Full }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "upgrade",
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": " ",
          "foreground": "#434C5E",
          "background": "#EBCB8B"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#5E81AC",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }}<#6CA35E> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#ff8080> \u25cf {{ .Working.String }}</>{{ end }}",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "trailing_diamond": "",
          "foreground": "#2e9599",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#BF616A{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " Ôì° <background,transparent></>",
          "type": "status"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "trailing_diamond": "",
          "foreground": "#2e9599",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#BF616A{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " Ó¨¨ <background,transparent></>",
          "type": "status"
        }
      ],
      "newline": true
    }
  ]
}
