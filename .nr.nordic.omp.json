{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{.Folder}}{{if .Root}} | root{{end}} - {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ECEFF4",
          "background": "#D08770",
          "template": " \udb86\udc53 "
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ECEFF4",
          "background": "#ff8080",
          "template": "  {{ .Context }} "
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#4C566A",
          "background": "#ECEFF4",
          "template": " {{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#434C5E",
          "background": "#D8DEE9",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\udb80\udede",
            "style": "agnoster"
          }
        },
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ECEFF4",
          "background": "#ff8080",
          "template": " {{ reason .Code }} "
        },
        {
          "type": "angular",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#5E81AC",
          "background": "#2E3440",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#6CA35E",
          "background": "#2E3440",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": " ",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "󰍟",
          "foreground": "#ECEFF4",
          "background": "#BF616A",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#434C5E",
          "background": "#EBCB8B",
          "leading_diamond": "",
          "trailing_diamond": "󰍟 ",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} \ue0b1 {{ end }}{{ .Full }}{{ end }}"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "upgrade",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#434C5E",
          "background": "#EBCB8B"
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "foreground": "#434C5E",
          "background": "#5E81AC",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#EBCB8B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#6CA35E{{ end }}",
            "{{ if gt .Ahead 0 }}#ff8080{{ end }}",
            "{{ if gt .Behind 0 }}#ff8080{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#434C5E",
          "background": "#ECEFF4",
          "leading_diamond": "",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          }
        }
      ]
    }
  ]
}
