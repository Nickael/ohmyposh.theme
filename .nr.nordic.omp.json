{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{.Folder}}{{if .Root}} | root{{end}} - {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#D08770",
          "template": " \udb86\udc53 "
        },
        {
          "type": "docker",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#ff8080",
          "template": "  {{ .Context }} "
        },
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#434C5E",
          "template": " {{ .UserName }} "
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#434C5E",
          "background": "#D8DEE9",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\udb80\udede",
            "style": "agnoster"
          }
        },
        {
          "type": "angular",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#88C0D0",
          "template": "  {{ .Full }} "
        },
        {
          "type": "npm",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#193549",
          "background": "#BF616A",
          "template": " {{ .Full }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#2E3440",
          "background": "#A3BE8C",
          "template": "  {{ .Full }} "
        },
        {
          "type": "status",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#BF616A",
          "template": " {{ reason .Code }} "
        }
      ]
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "upgrade",
          "style": "diamond",
          "leading_diamond": "\ue0c7",
          "trailing_diamond": "",
          "foreground": "#ECEFF4",
          "background": "#EBCB8B"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "final_space": true,
      "version": 2,
      "segments": [
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#434C5E",
          "template": "<#ffffff> </>{{ .HEAD }}{{ if .Staging.Changed }}<#6CA35E> \udb86\udc53 {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#ff8080> \udb86\udc53 {{ .Working.String }}</>{{ end }}<#ffffff> </>",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#BF616A",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#434C5E",
          "background": "#EBCB8B",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} \ue0b1 {{ end }}{{ .Full }}{{ end }} "
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " \ueb2c ",
          "foreground": "#ECEFF4",
          "template": " {{ if .WSL }}\uebca {{ end }}{{.Icon}}",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          }
        }
      ],
      "type": "prompt"
    }
  ]
}
