{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{.Folder}}{{if .Root}} | root{{end}} - {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#D08770",
          "template": " \udb86\udc53 "
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#ECEFF4",
          "background": "#ff8080",
          "template": "  {{ .Context }} "
        },
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#4C566A",
          "background": "#ECEFF4",
          "template": " {{ .UserName }} "
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#434C5E",
          "background": "#D8DEE9",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\udb80\udede",
            "style": "agnoster"
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "foreground": "#ECEFF4",
          "background": "#ff8080",
          "template": " {{ reason .Code }} "
        },
        {
          "type": "angular",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#88C0D0",
          "template": "  {{ .Full }} "
        },
        {
          "type": "npm",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#193549",
          "background": "#BF616A",
          "template": " {{ .Full }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#2E3440",
          "background": "#A3BE8C",
          "template": "  {{ .Full }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "final_space": true,
      "version": 2,
      "segments": [
        {
          "type": "upgrade",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#434C5E",
          "background": "#EBCB8B"
        },
        {
          "type": "angular",
          "style": "diamond",
          "trailing_diamond": "",
          "foreground": "#5E81AC",
          "background": "#434C5E",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#6CA35E",
          "background": "#434C5E",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#ECEFF4",
          "background": "#BF616A",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "foreground": "#434C5E",
          "background": "#EBCB8B",
          "leading_diamond": "",
          "trailing_diamond": "󰍟 ",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} \ue0b1 {{ end }}{{ .Full }}{{ end }}"
        },
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#434C5E",
          "background": "#ECEFF4",
          "leading_diamond": "",
          "trailing_diamond": "\ue0c6",
          "invert_powerline": true,
          "foreground": "#193549",
          "background": "#8FBCBB",
          "template": " {{ .HEAD }} "
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ueb2c",
          "foreground": "#ECEFF4",
          "template": " {{ if .WSL }}WSL {{ end }}{{.Icon}} ",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          }
        }
      ],
      "type": "prompt"
    }
  ]
}
