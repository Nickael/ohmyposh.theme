{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.Folder}}{{if .Root}} | root{{end}} - {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "trailing_diamond": "<#5E81AC, transparent>\u2592</>",
          "foreground": "#D8DEE9",
          "background": "#4C566A",
          "type": "session",
          "templates": [
            "{{ if .Root }}<#D08770,#3B4252> \udb86\udc53 </>{{ end }}",
            "{{ if .SSHSession }} üëæ{{ end }}",
            " {{ .UserName }} "
          ]
        },
        {
          "properties": {
            "folder_icon": "\ue5fe",
            "folder_separator_icon": " <#4C566A, transparent>\ue0b1</> ",
            "home_icon": "üè†",
            "mapped_locations": {
              "/home/nr/Workspaces": "üëæ"
            },
            "style": "mixed"
          },
          "style": "diamond",
          "trailing_diamond": "<#4C566A, transparent>\ue0b0</>",
          "template": " {{ .Path }} ",
          "foreground": "#ECEFF4",
          "background": "#3B4252",
          "type": "path"
        },
        {
          "style": "diamond",
          "trailing_diamond": "<#58647A, transparent>\ue0b0</>",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#BF616A",
          "background": "#3B4252",
          "type": "ruby"
        },
        {
          "style": "diamond",
          "trailing_diamond": "<#58647A, transparent>\ue0b0</>",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#EBCB8B",
          "background": "#3B4252",
          "type": "python"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "style": "plain",
          "template": " [ <#58647A,transparent>{{.Current}}</> -> <#A3BE8C,transparent>{{.Latest}}</> ] ",
          "foreground": "",
          "background": "",
          "type": "upgrade"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "trailing_diamond": "",
          "template": "<#ffffff> </>{{ .HEAD }}{{ if .Staging.Changed }}<#6CA35E> \udb86\udc53 {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#ff8080> \udb86\udc53 {{ .Working.String }}</>{{ end }}<#ffffff> </>",
          "foreground": "#ECEFF4",
          "background": "",
          "type": "git",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          },
          "style": "diamond",
          "foreground": "#ef5350",
          "background": "",
          "type": "shell"
        },
        {
          "properties": {
            "branch_icon": "üå≤ ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "foreground": "#5E81AC",
          "background": "",
          "type": "git",
          "templates": [
            "<#2e9599,transparent>{{ .HEAD }}</>"
          ]
        },
        {
          "style": "powerline",
          "template": " \ue753 {{ .Full }} ",
          "foreground": "#3B4252",
          "powerline_symbol": "\ue0b0",
          "background": "#5E81AC",
          "type": "nx"
        },
        {
          "style": "diamond",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#5E81AC",
          "background": "",
          "type": "angular"
        },
        {
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          },
          "style": "diamond",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#6CA35E",
          "background": "",
          "type": "node"
        },
        {
          "style": "diamond",
          "template": " \uf308 {{ .Context }} ",
          "foreground": "#348cba",
          "background": "",
          "type": "docker"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \ueb2c <background, transparent></>",
          "foreground": "#2e9599",
          "background": "",
          "type": "status",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#BF616A{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
